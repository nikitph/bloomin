{
  "CVE-2021-44228": {
    "name": "Log4Shell",
    "severity": "CRITICAL",
    "description": "Remote code execution via JNDI injection in Log4j",
    "stages": [
      {
        "stage": 1,
        "mitre_technique": "T1190",
        "technique_name": "Exploit Public-Facing Application",
        "description": "Initial JNDI injection attempt",
        "patterns": [
          "${jndi:ldap://",
          "${jndi:rmi://",
          "${jndi:dns://",
          "${jndi:iiop://",
          "${jndi:corba://",
          "${jndi:ldaps://",
          "${lower:jndi:",
          "${upper:jndi:",
          "$%7Bjndi:",
          "${${::-j}ndi:"
        ]
      },
      {
        "stage": 2,
        "mitre_technique": "T1059",
        "technique_name": "Command and Scripting Interpreter",
        "description": "Remote code execution after payload delivery",
        "patterns": [
          "Runtime.exec",
          "ProcessBuilder",
          "/bin/bash -c",
          "/bin/sh -c",
          "powershell.exe",
          "cmd.exe /c",
          "wget http://",
          "curl http://",
          "Invoke-Expression",
          "eval("
        ]
      },
      {
        "stage": 3,
        "mitre_technique": "T1053",
        "technique_name": "Scheduled Task/Job",
        "description": "Persistence mechanism installation",
        "patterns": [
          "crontab @reboot",
          "crontab -e",
          "systemctl enable",
          "systemctl start",
          "schtasks /create",
          "at 00:00",
          "/etc/rc.local",
          ".bashrc",
          ".bash_profile"
        ]
      },
      {
        "stage": 4,
        "mitre_technique": "T1041",
        "technique_name": "Exfiltration Over C2 Channel",
        "description": "Data exfiltration or C2 communication",
        "patterns": [
          "POST /upload",
          "base64 -d",
          "gzip -c",
          "tar czf",
          "scp -r",
          "rsync -avz",
          "curl -X POST",
          "wget --post-file"
        ]
      }
    ]
  },
